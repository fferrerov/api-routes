{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "ffv/ffv-api-routes/(.*)": "<rootDir>/../libs/ffv-api-routes/src/$1",
    "ffv/ffv-api-routes": "<rootDir>/../libs/ffv-api-routes/src"
  }
}